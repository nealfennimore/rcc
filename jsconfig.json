{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./",
        "paths": {
            "src/*": [
                "src/*"
            ],
            "global/*": [
                "src/global/*"
            ],
            "pages/*": [
                "src/pages/*"
            ],
        }
    },
    "include": [
        "src",
    ],
    "exclude": [
        "**/node_modules/*",
        "**/dist/*"
    ]
}